      for (let y = 0; y < ctx.height; y++) {
        for (let x = 0; x < ctx.width; x++) {
          const dx = x - cx;
          const dy = y - cy;
          const dist = Math.sqrt(dx * dx + dy * dy);
          if (dist <= rad && dist >= rad - 3) {
            setPixel(out, x, y, r, g, b);
          }
        }
      }
      return out;
