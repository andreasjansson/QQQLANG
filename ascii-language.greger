        out.data[i] = Math.min(255, r * 0.393 + g * 0.769 + b * 0.189);
        out.data[i + 1] = Math.min(255, r * 0.349 + g * 0.686 + b * 0.168);
        out.data[i + 2] = Math.min(255, r * 0.272 + g * 0.534 + b * 0.131);
