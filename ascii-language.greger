    function grayscale(ctx) {
      const prev = getPrevImage(ctx);
      const out = cloneImage(prev);
      for (let i = 0; i < out.data.length; i += 4) {
        const gray = Math.round(out.data[i] * 0.299 + out.data[i + 1] * 0.587 + out.data[i + 2] * 0.114);
        out.data[i] = gray;
        out.data[i + 1] = gray;
