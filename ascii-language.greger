      const thresh = n * 2.55;
      for (let i = 0; i < out.data.length; i += 4) {
        if (out.data[i] > thresh) out.data[i] = 255 - out.data[i];
        if (out.data[i + 1] > thresh) out.data[i + 1] = 255 - out.data[i + 1];
        if (out.data[i + 2] > thresh) out.data[i + 2] = 255 - out.data[i + 2];
