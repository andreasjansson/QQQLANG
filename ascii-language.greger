      for (let y = 0; y < ctx.height; y++) {
        for (let x = 0; x < ctx.width; x++) {
          const sx = (x / ctx.width) * prev.width;
          const sy = Math.pow(y / ctx.height, factor) * prev.height;
          const px = bilinearSample(prev, sx, sy);
          setPixel(out, x, y, px[0], px[1], px[2]);
