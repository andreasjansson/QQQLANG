                const px = getPixel(img, x + kx, y + ky);
                r += px[0];
                g += px[1];
                b += px[2];
                count++;
              }
            }
            setPixel(out, x, y, r / count, g / count, b / count);
          }
        }
        img = out;
      }
      return img;
    }

