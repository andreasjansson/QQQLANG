          const dx = (x - cx) / maxR;
          const dy = (y - cy) / maxR;
          const r = Math.sqrt(dx * dx + dy * dy);
          if (r < 1) {
            const z = Math.sqrt(1 - r * r);
            const sx = (cx + dx * (1 - z) * maxR) / ctx.width * prev.width;
