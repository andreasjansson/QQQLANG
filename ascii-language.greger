        const gray = out.data[i] * 0.299 + out.data[i + 1] * 0.587 + out.data[i + 2] * 0.114;
        out.data[i] = gray + (out.data[i] - gray) * factor;
        out.data[i + 1] = gray + (out.data[i + 1] - gray) * factor;
        out.data[i + 2] = gray + (out.data[i + 2] - gray) * factor;
