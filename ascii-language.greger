      for (let y = 0; y < ctx.height; y++) {
        for (let x = 0; x < ctx.width; x++) {
          const dx = Math.abs((x % size) - size / 2);
          const dy = Math.abs((y % size) - size / 2);
          if (dx + dy < size / 2) {
            const px = getPixel(prev, x, y);
            setPixel(out, x, y, px[0], px[1], px[2]);
