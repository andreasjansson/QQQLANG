          const dx = x - cx;
          const dy = y - cy;
          const r = Math.sqrt(dx * dx + dy * dy);
          const nr = r / maxR;
          const factor = nr < 1 ? Math.pow(Math.sin(Math.PI * nr / 2), strength) : 1;
          const sx = (cx + dx * factor) / ctx.width * prev.width;
          const sy = (cy + dy * factor) / ctx.height * prev.height;
