          const ox = Math.floor((x / ctx.width) * old.width);
          const oy = Math.floor((y / ctx.height) * old.height);
          const oidx = (oy * old.width + ox) * 4;
          for (let c = 0; c < 3; c++) {
            out.data[idx + c] = Math.abs(out.data[idx + c] - old.data[oidx + c]);
          }
        }
