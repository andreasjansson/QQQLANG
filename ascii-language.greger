      const [r, g, b] = getPixel(prev, x, y);
      const luminance = (r * 0.299 + g * 0.587 + b * 0.114) / 255;
      const spacing = Math.max(2, Math.floor(2 + luminance * 15));
      
      const diag = ((ctx.width - 1 - x) + y) % spacing;
      if (diag === 0) {
