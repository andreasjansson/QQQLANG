          const src = (qx === qy) ? prev : old;
          const lx = x < hw ? x : x - hw;
          const ly = y < hh ? y : y - hh;
          const sx = (lx / hw) * src.width;
          const sy = (ly / hh) * src.height;
          const [r, g, b, a] = bilinearSample(src, sx, sy);
